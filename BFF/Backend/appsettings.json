{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "youtube-api": {
        "ClusterId": "youtube",
        "Match": {
          "Path": "/yarp-api-yt/{**rest}"
        },
        "Transforms": [
          { "PathPattern": "/connect/{**rest}" }
        ]
      }
    },
    "Clusters": {
      "youtube": {
        "Destinations": {
          "destination1": {
            "Address": "http://192.168.8.109:7211"
          }
        }
      }
    }
  }
}
